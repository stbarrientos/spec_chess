<%= form_for @test do |t| %>
<p>
	<%= t.label :name %>
	<%= t.text_field :name %>
</p>
<p>
	<%= t.label :personal %>
	<%= t.radio_button :personal, true %>
	<%= t.radio_button :personal, false %>
</p>
<p>
	<%= t.label :description %>
	<%= t.text_field :description %>
</p>
<p>
	<%= t.label :status %>
	<%= t.text_field :status %>
</p>
<p>
	<%= t.label :code %>
	<%= t.text_area :code %>
</p>
<p>
	<%= t.submit %>
</p>

<% end %>